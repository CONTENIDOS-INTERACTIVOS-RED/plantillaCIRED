EAAA;AAFA,KAlBA;AAsBA,IAAA,cAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA;AAtBA,GAFA;AA6BA,EAAA,IA7BA,kBA6BA;AACA,WAAA;AACA,MAAA,iBAAA,EAAA;AADA,KAAA;AAGA,GAjCA;AAkCA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,UAAA,UAAA,GAAA;AACA,QAAA,SAAA,EAAA,CADA;AAEA,QAAA,WAAA,EAAA,CAFA;AAGA,QAAA,KAAA,EAAA,KAAA,UAAA,CAAA,MAHA;AAIA,QAAA,UAAA,EAAA;AAJA,OAAA;AAMA,WAAA,UAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,UAAA,EAAA;AACA,UAAA,UAAA,CAAA,SAAA;AACA,SAFA,MAEA;AACA,UAAA,UAAA,CAAA,WAAA;AACA;AACA,OANA;AAOA,MAAA,UAAA,CAAA,UAAA,GAAA,UAAA,CAAA,SAAA,GAAA,UAAA,CAAA,KAAA,GAAA,GAAA;AACA,aAAA,UAAA;AACA,KAjBA;AAkBA,IAAA,QAlBA,sBAkBA;AACA,aAAA,KAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA;AApBA,GAlCA;AAwDA,EAAA,OAxDA,qBAwDA;AACA,SAAA,eAAA;AACA,GA1DA;AA2DA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AAAA;;AACA,UAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,GAAA,WAAA,CAAA,GAAA,EAAA;;AAEA,UAAA,OAAA,GAAA,SAAA,OAAA,CAAA,IAAA,EAAA;AACA,YAAA,YAAA,GAAA,CAAA,IAAA,GAAA,KAAA,IAAA,QAAA;;AACA,YAAA,YAAA,GAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,CAAA,mBAAA;AACA,SAFA,MAEA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,YAAA,GAAA,KAAA,CAAA,mBAAA;AACA,UAAA,qBAAA,CAAA,OAAA,CAAA;AACA;AACA,OARA;;AAUA,MAAA,qBAAA,CAAA,OAAA,CAAA;AACA;AAhBA;AA3DA,CAAA","sourcesContent":["<template>\n  <div class=\"row align-items-center justify-content-around\">\n    <div class=\"col-lg-5\">\n      <img\n        v-if=\"aprobado\"\n        src=\"@/assets/actividad/img_correcto.svg\"\n        alt=\"\"\n        class=\"mx-auto d-none d-lg-block\"\n      />\n      <img\n        v-else\n        src=\"@/assets/actividad/img_incorrecto.svg\"\n        alt=\"\"\n        class=\"mx-auto d-none d-lg-block\"\n      />\n    </div>\n    <div class=\"col-lg-4\">\n      <div class=\"d-flex align-items-center flex-column\">\n        <div class=\"circle-container mb-4\">\n          <svg class=\"circle\" viewBox=\"0 0 36 36\">\n            <defs>\n              <linearGradient\n                id=\"progressGradient\"\n                x1=\"0%\"\n                y1=\"0%\"\n                x2=\"100%\"\n                y2=\"0%\"\n              >\n                <stop offset=\"0%\" stop-color=\"#00FF00\" />\n                <stop offset=\"50%\" stop-color=\"#FFFF00\" />\n                <stop offset=\"100%\" stop-color=\"#FF0000\" />\n              </linearGradient>\n            </defs>\n            <path\n              class=\"circle-bg\"\n              d=\"M18 2.0845\n                a 15.9155 15.9155 0 0 1 0 31.831\n                a 15.9155 15.9155 0 0 1 0 -31.831\"\n            />\n            <path\n              class=\"circle-progress\"\n              :stroke-dasharray=\"`${currentPercentage}, 100`\"\n              d=\"M18 2.0845\n                a 15.9155 15.9155 0 0 1 0 31.831\n                a 15.9155 15.9155 0 0 1 0 -31.831\"\n            />\n          </svg>\n          <div class=\"percentage-text\">\n            {{ Math.round(porcentajeAprobadas) }}%\n          </div>\n        </div>\n\n        <template v-if=\"aprobado\">\n          <h3>¡BUEN TRABAJO!</h3>\n          <p class=\"mb-0 text-center\">\n            {{ mensajeAprobado }}\n          </p>\n        </template>\n        <template v-else>\n          <h3>VUELVE A INTENTARLO</h3>\n          <p class=\"mb-0 text-center\">\n            {{ mensajeReprobado }}\n          </p>\n        </template>\n        <hr class=\"w-100\" />\n        <p class=\"mb-0\">Aciertos: {{ rtas.correctas }} / {{ rtas.total }}</p>\n        <button\n          v-if=\"totalPreguntasBase >= preguntasCount\"\n          class=\"boton btn-lg boton--b py-3 px-5 mt-3\"\n          @click=\"$emit('reiniciar')\"\n        >\n          <span>Volver a intentarlo</span>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ActividadResultados',\n  props: {\n    respuestas: {\n      type: Array,\n      required: true,\n    },\n    mensajeAprobado: {\n      type: String,\n      default: '¡Excelente! Ha superado la actividad.',\n    },\n    mensajeReprobado: {\n      type: String,\n      default:\n        'Le recomendamos volver a revisar el componente formativo e intentar nuevamente la actividad didáctica.',\n    },\n    porcentajeAprobadas: {\n      type: Number,\n      required: true,\n    },\n    totalPreguntasBase: {\n      type: Number,\n      required: true,\n    },\n    preguntasCount: {\n      type: Number,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      currentPercentage: 0,\n    }\n  },\n  computed: {\n    rtas() {\n      const respuestas = {\n        correctas: 0,\n        incorrectas: 0,\n        total: this.respuestas.length,\n        porcentaje: 0,\n      }\n      this.respuestas.forEach(r => {\n        if (r.esCorrecta) {\n          respuestas.correctas++\n        } else {\n          respuestas.incorrectas++\n        }\n      })\n      respuestas.porcentaje = (respuestas.correctas / respuestas.total) * 100\n      return respuestas\n    },\n    aprobado() {\n      return this.rtas.porcentaje >= 70\n    },\n  },\n  mounted() {\n    this.animateProgress()\n  },\n  methods: {\n    animateProgress() {\n      const duration = 1500\n      const start = performance.now()\n\n      const animate = time => {\n        const timeFraction = (time - start) / duration\n        if (timeFraction > 1) {\n          this.currentPercentage = this.porcentajeAprobadas\n        } else {\n          this.currentPercentage = timeFraction * this.porcentajeAprobadas\n          requestAnimationFrame(animate)\n        }\n      }\n\n      requestAnimationFrame(animate)\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\" scoped>\n\n\n.resultados\n  &__icono\n    width: 150px\n    height: 150px\n    background-size: cover\n    background-repeat: no-repeat\n    &--correcto\n      background-image: url('~@/assets/actividad/correcto.svg')\n    &--incorrecto\n      background-image: url('~@/assets/actividad/incorrecto.svg')\n    @media (max-width: 560px)\n      width: 80px\n      height: 80px\n\n.circle-container\n  width: 150px\n  height: 150px\n  position: relative\n  display: flex\n  justify-content: center\n  align-items: center\n\n.circle\n  transform: rotate(0deg)\n  width: 100%\n  height: 100%\n\n.circle-bg\n  fill: none\n  stroke: #eee\n  stroke-width: 3.8\n\n.circle-progress\n  fill: none\n  stroke: url(#progressGradient)\n  stroke-width: 3.8\n  stroke-linecap: round\n  transition: stroke-dasharray 0.1s\n\n.percentage-text\n  position: absolute\n  font-size: 1.5rem\n  font-weight: bold\n  color: #666\n</style>\n"],"sourceRoot":"src/components/actividad"}]}